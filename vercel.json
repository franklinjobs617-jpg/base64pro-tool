{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.base64pro.top" }
      ],
      "destination": "https://base64pro.top/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, max-age=0, must-revalidate" }
      ]
    }
  ]
}
